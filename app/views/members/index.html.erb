<p style="color: green"><%= notice %></p>

<h1>Members</h1>

<div id="members">
  <% @members.each do |member| %>
    <%= render member %>
    <p>
      <%= link_to "Show this member", member, class:"btn btn-secondary" %>
    </p>
  <% end %>
</div>

<%= link_to "New member", new_member_path, class:"btn btn-success" %>
